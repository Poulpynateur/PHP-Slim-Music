<aside class="cache-misere">
    <div class="aside-menu">
        <span id="aside_menu_toggler">
            <svg class="mr-0 mr-md-5" width="50" height="50" xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 100 100">
                <polyline points="15,50 50,85 85,50" fill="none" stroke="#000000" stroke-width="10" stroke-linecap="round"></polyline>
            </svg>
        </span>
        <ul id="aside_menu_list" class="nav flex-column bg-white">
            {% for folder in folders %}
            <li class="nav-item">
                <a class="nav-link dropdown-item" href="#music_folder_{{ folder.title }}">{{ folder.title }}</a>
            </li>
            {% endfor %}
        </ul>
    </div>
</aside>